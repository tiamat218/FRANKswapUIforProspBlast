open) round\nif(epoch===currentEpoch&&lockPrice===null){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(RoundCard_OpenRoundCard,{round:round,hasEnteredDown:hasEnteredDown,hasEnteredUp:hasEnteredUp,betAmount:ledger===null||ledger===void 0?void 0:ledger.amount,bullMultiplier:formattedBullMultiplier,bearMultiplier:formattedbearMultiplier});}// Live round\nif(closePrice===null&&epoch===currentEpoch-1){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(RoundCard_LiveRoundCard,{betAmount:ledger===null||ledger===void 0?void 0:ledger.amount,hasEnteredDown:hasEnteredDown,hasEnteredUp:hasEnteredUp,round:round,bullMultiplier:formattedBullMultiplier,bearMultiplier:formattedbearMultiplier});}// Fake future rounds\nif(epoch>currentEpoch){return/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(RoundCard_SoonRoundCard,{round:round});}// Past rounds\nreturn/*#__PURE__*/Object(jsx_runtime[\"jsx\"])(RoundCard_ExpiredRoundCard,{round:round,hasEnteredDown:hasEnteredDown,hasEnteredUp:hasEnteredUp,hasClaimedDown:hasClaimedDown,hasClaimedUp:hasClaimedUp,betAmount:ledger===null||ledger===void 0?void 0:ledger.amount,bullMultiplier:formattedBullMultiplier,bearMultiplier:formattedbearMultiplier});};/* harmony default export */ var components_RoundCard = (RoundCard);\n// CONCATENATED MODULE: ./src/views/Predictions/components/Label.tsx\nvar Label_templateObject,Label_templateObject2,Label_templateObject3,Label_templateObject4,Label_templateObject5;const Token=Object(styled_components_browser_esm[\"e\" /* default */])(index_esm[\"n\" /* Box */])(Label_templateObject||(Label_templateObject=Object(taggedTemplateLiteral[\"a\" /* default */])([\"\\n  margin-top: -24px;\\n  position: absolute;\\n  top: 50%;\\n  z-index: 30;\\n\\n  & > svg {\\n    height: 48px;\\n    width: 48px;\\n  }\\n\\n  \",\" {\\n    margin-top: -32px;\\n\\n    & > svg {\\n      height: 64px;\\n      width: 64px;\\n    }\\n  }\\n\"])),_ref=>{let{theme}=_ref;return theme.mediaQueries.lg;});const Title=Object(styled_components_browser_esm[\"